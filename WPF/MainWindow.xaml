<Window x:Class="WPF.MainWindow"
       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        xmlns:local="clr-namespace:WPF"
        mc:Ignorable="d"
        Title="GPS Location" Height="560" Width="954">
    <DockPanel LastChildFill="True">
        <Border Background="#FF332B2B" Width="100">
            <Button x:Name="LogoutButton" Content="Logout" Click="LogoutButton_Click" Margin="10,482,10,10"/>
        </Border>
        <Border DockPanel.Dock="Top" Background="#FF332B2B" Height="58">
            <TextBlock x:Name="UserTestText"
                       Text=""
                       VerticalAlignment="Center"
                       Margin="10,0,0,0"
                       Foreground="White"
                       FontSize="24"
                       FontWeight="Medium"/>
        </Border>
        <wv2:WebView2 x:Name="MyWebView" />
    </DockPanel>
</Window>
